<h1><%= @genre.name %> </h1>

<label>Songs:</label>
<ul>
    <% @genre.songs.each do |song|%>
    <li><a href="/songs/<%= song.slug %>"><%= song.name %>
    </a></li>
    <% end %>
</ul>

<label>Artists:</label>
<ul>
    <% @genre.artists.each do |artist|%>
    <li><a href="/artists/<%= artist.slug %>"><%= artist.name %>
    </a></li>
    <% end %>
</ul>